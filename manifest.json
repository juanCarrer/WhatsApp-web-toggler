{
  "name": "Juancarrer - wsp",
  "description": "extension para ocultar elementos de wsp web",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["./contentScript.js"]
    }
  ],
  "icons": {
    "16": "./icons/wsp_icon.png",
    "32": "./icons/wsp_icon.png",
    "48": "./icons/wsp_icon.png",
    "128": "./icons/wsp_icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": []
}